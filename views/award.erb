<div class="col-md-3">
  <div class="award">

    <div class="over">
      <img class="ribbon" src="/assets/pink_ribbon.png" alt="">
    </div>

    <div class="top votes">
      <% if award.votable? %>
        <form action="/nominations/<%= award.id %>/vote" method="post">
          <input type="submit" value="<%= award.votes_count %>" class="votes">
        </form>
      <% else %>
        <div class="votes"><%= award.votes_count %></div>
      <% end %>
    </div>

    <div class="under">
      <img class="prof-pic" src="<%= award.nominee.pic_url %>" alt="">
    </div>

    <div>
      <h2 class="launcher-name"><%= award.nominee.display_name %></h2>
    </div>

    <h2 class="award-title"><%= award.content %></h2>

  </div>
</div>
