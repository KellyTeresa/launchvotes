<div class="col-md-3">
  <div class="award">

    <div class="over">
      <img class="ribbon" src="../assets/pink_ribbon.png">
    </div>

    <div class="top votes">
      <form action="/nominations/<%= award.id %>/vote" method="post">
        <input type="submit" value="Vote" class="votes">
      </form>
    </div>

    <div class="under">
      <img class="prof-pic" src="<%= award.nominee.pic_url %>">
    </div>

    <div>
      <h2 class="launcher-name"><%= award.nominee.name %></h2>
    </div>

    <h2 class="award-title"><%= award.content %></h2>

  </div>
</div>
